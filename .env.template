# Optional keys
# UEXCorp - https://uexcorp.space/api/documentation/
UEXCORP_API_KEY=...
# StarCitizen-API - https://starcitizen-api.com/
SCAPI_KEY=...

# Discord keys for Bot and Application - https://discord.com/developers/applications
DISCORD_CLIENT_ID=...
DISCORD_CLIENT_SECRET=...
DISCORD_API_KEY=...

# Required setting
SESSION_SECRET=...

# Path
IMAGE_LAMBDA_NAME=sc-market-image-lambda
BACKEND_URL=http://localhost:8080
CDN_URL=https://cdn.sc-market.space
BACKEND_PORT=8080
FRONTEND_URL=http://localhost:5173
DISCORD_BACKEND_URL=http://localhost:8081
DISCORD_BOT_URL=http://127.0.0.1:8080

# Database Pass - Should match docker-compose.yml
DATABASE_PASS={"username":"scmarket","password":"scmarket","dbname":"scmarket","port":5432,"host":"localhost"}

# For local development
NODE_ENV=development

# B2 Access Keys
B2_KEY_ID=
B2_APP_KEY=
B2_BUCKET_NAME=
# SQS Queue URLs for Discord bot communication
DISCORD_QUEUE_URL=
BACKEND_QUEUE_URL=

# SQS-specific AWS Credentials (separate from other AWS services)
BACKEND_ACCESS_KEY_ID=
BACKEND_SECRET_ACCESS_KEY=
AWS_REGION=us-east-2
SQS_ROLE_ARN=

# Push Notifications (Web Push Protocol)
# Generate VAPID keys using: npx web-push generate-vapid-keys
# The public key should also be set in the frontend .env as VITE_VAPID_PUBLIC_KEY
VAPID_PUBLIC_KEY=
VAPID_PRIVATE_KEY=
VAPID_SUBJECT=mailto:admin@sc-market.space
